<div class="container">
  {{navbar-user user=model}}
   

   <div class="col-lg-12" id="nameTeamContainer">  
     <div class="thumbnail" id="nameTeam">
       <h1>{{model.name}}</h1>
     </div>
  </div>

  <div class="col-lg-12">
   {{#if auctionCreated}}
     <div class="alert alert-success"><b>{{t 'success'}}!</b> {{t 'createdAuction'}}.</div>
   {{/if}}

   {{#if errorMessage}}
     <div class="alert alert-danger"><b>{{t 'error'}}</b>! {{t 'errors.createAuction'}}.</div>
   {{/if}}
   </div>

  <div class="row">
    {{#each contract in model.contracts}}
      {{partial "contracts/panel"}}
    {{/each}}
  </div>